    "express": "4.16.3",
    "glob": "7.1.2",
    "gray-matter": "4.0.1",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "5.0.0",
    "gulp-clean": "0.4.0",
    "gulp-connect": "5.5.0",
    "gulp-convert-encoding": "1.0.0",
    "gulp-data": "1.3.1",
    "gulp-ejs": "3.1.3",
    "gulp-eslint": "5.0.0",
    "gulp-front-matter": "1.3.0",
    "gulp-header": "2.0.5",
    "gulp-htmlhint": "2.1.1",
    "gulp-if": "2.0.2",
