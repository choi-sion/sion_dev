  "scripts": {
    "start": "nodemon app.js",
    "build": "gulp",
    "server": "gulp server"
  },
  "engines": {
    "node": "8.x.x",
    "npm": ">=5.1.0"
  },
  "browserslist": [
    "iOS > 7",
    "Android >= 4.1",
    "Chrome > 40",
    "ChromeAndroid > 50",
    "FirefoxAndroid >= 46",
    "OperaMobile >= 37"
  ],
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules",
      "dist",
      "src"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js json"
  },
  "htmlBeautify": {
    "end_with_newline": true,
    "unformatted": true,
    "preserve_newlines": false,
    "extra_liners": [],
    "indent_size": 2
  }
}
