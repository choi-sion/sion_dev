{
  "name": "sion",
  "siteName": "sion",
  "description": "sion",
  "homepage": "https://gitlab.com/webPublisher/sion/",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/webPublisher/sion.git"
  },
  "team": "UI Developer",
  "license": "SEE LICENSE IN LICENSE.txt",
  "private": true,
  "port": 8090,
  "base": {
    "src": "src",
    "dist": "dist",
    "data": "data"
  },
  "viewport": {
    "width": "device-width",
    "initialScale": 1
  },
  "xUaCompatible": false,
  "formatDetection": {
    "telephone": "no",
    "date": "no",
    "address": "no",
    "email": "no"
  },
  "dependencies": {
    "autoprefixer": "9.4.7",
    "del": "3.0.0",
    "ejs": "2.6.1",
    "express": "4.16.4",
    "fancy-log": "1.3.3",
    "glob": "7.1.3",
    "gray-matter": "4.0.2",
    "gulp": "^4.0.0",
    "gulp-connect": "5.7.0",
    "gulp-convert-encoding": "2.0.1",
    "gulp-data": "1.3.1",
    "gulp-ejs": "3.3.0",
    "gulp-eslint": "5.0.0",
    "gulp-front-matter": "1.3.0",
    "gulp-header": "2.0.7",
    "gulp-htmlhint": "2.2.1",
    "gulp-load-plugins": "1.5.0",
    "gulp-pluck": "0.0.4",
    "gulp-postcss": "8.0.0",
    "gulp-prettify": "0.5.0",
    "gulp-rename": "1.4.0",
    "gulp-sass": "4.0.2",
    "gulp-sass-lint": "1.4.0",
    "js-beautify": "1.8.9",
    "mime-types": "2.1.21",
    "node-sass-middleware": "0.11.0",
    "nodemon": "1.18.9",
    "postcss-middleware": "1.1.4",
    "yargs": "12.0.5"
  },
  "scripts": {
    "start": "nodemon app.js",
    "build": "gulp",
    "server": "gulp server"
  },
  "engines": {
    "node": "8.x.x",
    "npm": ">=5.1.0"
  },
  "languages": [
    "ko"
  ],
  "browserslist": [
    "iOS > 7",
    "Android >= 4.1",
    "Chrome > 40",
    "ChromeAndroid > 50",
    "FirefoxAndroid >= 46",
    "OperaMobile >= 37"
  ],
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules",
      "dist",
      "src"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js json"
  },
  "htmlBeautify": {
    "end_with_newline": true,
    "unformatted": true,
    "preserve_newlines": true,
    "max_preserve_newlines": 0,
    "indent_inner_html": true,
    "extra_liners": [],
    "indent_size": 2,
    "brace_style": "collapse",
    "wrap_line_length": 100
  }
}
